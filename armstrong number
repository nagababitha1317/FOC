#include<stdio.h>
#include<conio.h>

int main()
{
	int a,b,c,d,e;
	printf("Enter the value of a: ");
	scanf("%d",&a);
	e=a;
	while(a>0)
	{
		b=a%10;
		c=b*b*b;
		a=a/10;
		d=c+d;
	}
	    if(e==d)
	     printf("the given number is armstrong",e);
	    else
	     printf("the given number is NOT armstrong",e);
	    getch();
}
